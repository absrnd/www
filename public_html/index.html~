<pre class="src src-emacs-lisp">(<span style="color: #a020f0;">defun</span> <span style="color: #0000ff;">create-links</span> ()
  (<span style="color: #a020f0;">let</span> ((post-list (directory-files <span style="color: #8b2252;">"~/projects/www.absrnd.com/org/_posts"</span> nil <span style="color: #8b2252;">"org$"</span>))
        (result))
    (<span style="color: #a020f0;">dolist</span> (post post-list result)
      (<span style="color: #a020f0;">setq</span> result (concat result
                           (concat <span style="color: #8b2252;">"[["</span>
                                   <span style="color: #8b2252;">"./_posts/"</span> post <span style="color: #8b2252;">".html]"</span>
                                   <span style="color: #8b2252;">"["</span> post <span style="color: #8b2252;">"]]"</span>))))))
(create-links)
</pre>
